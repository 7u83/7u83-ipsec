<%if @default_proposals %>
remote anonymous {
        exchange_mode main;
        generate_policy on;

        <%- @default_proposals.each do |p| -%>
	proposal {
                encryption_algorithm <%= p['encryption'] %>;
                hash_algorithm <%= p['hash'] %>;
                authentication_method pre_shared_key;
                dh_group <%= p['dh_group'] %>;
        }
        <%- end -%>
}
<% end %>

