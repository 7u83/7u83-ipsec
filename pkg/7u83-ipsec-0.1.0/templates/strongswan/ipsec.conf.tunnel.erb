
<% @nets.each do |net| -%>
conn <%= @title %>
        left=<%= @local_ip %>
	leftsubnet=<%= net['local'] %>
	right=<%= @remote_ip %>
        rightsubnet=<%= net['remote'] %>
        ike=aes256-md5-modp1024
        esp=aes256-md5-modp1024!
	auto=start
	authby=secret
        keyexchange=ikev1
<% end %>

